{
  "name": "@justmiracle/checkitout",
  "module": "index.ts",
  "type": "module",
  "private": false,
  "export": {
    ".": "index.ts"
  },
  "scripts": {
    "check": "biome check ./",
    "check:ci": "biome ci ./",
    "check:fix": "biome check --write --unsafe ./",
    "ci": "pnpm run check:ci && pnpm run type",
    "type": "tsc --noEmit --skipLibCheck",
    "build": "bun build --declaration --minify --target node --outdir dist index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
